              "type": "integer"
            }
          },
          {
            "description": "Custom string path id",
            "in": "path",
            "required": true,
            "name": "customId2",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "success"
          },
          "401": {
            "description": "not authenticated"
          },
          "500": {
            "description": "general error"
          }
        },
        "description": "A custom GET method",
        "summary": "",
        "tags": [
          "custom method service"
        ],
        "security": []
