            "name": "pathParamName",
            "schema": {
              "type": "string"
            },
            "required": true
          },
          {
            "description": "A defined param",
            "in": "query",
            "name": "test",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/custom_response"
                }
              }
            }
          },
          "401": {
            "description": "not authenticated"
          },
          "500": {
            "description": "general error"
          }
        },
        "description": "A custom POST method",
        "summary": "",
        "tags": [
          "custom method service"
        ],
        "security": [
          {
            "BearerAuth": []
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/custom_request"
              }
